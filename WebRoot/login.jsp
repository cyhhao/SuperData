<body>    
 <!--   <div id="loader"><img src="img/loader.gif"/></div> -->
       
    <div class="login">

        <div class="page-header">
            <div class="icon">
                <span class="ico-arrow-right"></span>
            </div>
            <h1>登录 <small>开启你的云数据挖掘时代</small></h1>
        </div>        
        
        <div class="row-fluid">
            <div class="row-form">
                <div class="span12">
                    <input type="text" name="login" placeholder="用户名"/>
                </div>
            </div>
            <div class="row-form">
                <div class="span12">
                    <input type="password" name="password" placeholder="密码" />
                </div>            
            </div>
            <div class="row-form">
                <div class="span12">
                    <input type="checkbox"/> 保持登录
                </div>
            </div>
            <div class="row-form">
                <div class="span12">
                    <a href="home"><button class="btn" style="font-size: medium;font-family: '微软雅黑'">登 录</button></a>
                </div>                
            </div>
        </div>
    </div>
    
